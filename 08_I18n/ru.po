#: guess.c:21
#, c-format
msgid "Does the number more than %d?\n"
msgstr "Число больше чем %d?\n"

#: guess.c:30
msgid "yes"
msgstr "да"

#: guess.c:30
msgid "y"
msgstr "д"

#: guess.c:32
msgid "no"
msgstr "нет"

#: guess.c:32
msgid "n"
msgstr "н"

#: guess.c:35
#, c-format
msgid "Bad input! Enter yes or no.\n"
msgstr "Неправильный ввод! Введите да или нет.\n"

#: guess.c:41
#, c-format
msgid "Your number is %d!\n"
msgstr "Ваше число -- %d!\n"
