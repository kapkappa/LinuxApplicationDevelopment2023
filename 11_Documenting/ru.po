#: guess.c:51
msgid "--help"
msgstr "--help"

#: guess.c:52
#, c-format
msgid "This is the help you asked!\n"
msgstr "Это сообщение о помощи о котором вы просили!\n"

#: guess.c:53
#, c-format
msgid ""
"Args usage:\n"
"\t--help shows this message\n"
"\t-r enables Roman numbers\n"
msgstr ""
"Использование аргументов:\n"
"\t--help показывает это сообщение\n"
"\t-r включает Римские числа\n"

#: guess.c:54
#, c-format
msgid "The program guesses the hidden number!\n"
msgstr "Программа отгадывает загаданное число!\n"

#: guess.c:57
msgid "-r"
msgstr "-r"

#: guess.c:58
#, c-format
msgid "Roman numbers enabled\n"
msgstr "Римские числа включены\n"

#: guess.c:61
#, c-format
msgid "WARNING: unknown argument!\n"
msgstr "ПРЕДУПРЕЖДЕНИЕ: неизвестный аргумент!\n"

#: guess.c:65
#, c-format
msgid "Please, guess a number from %s to %s.\n"
msgstr "Пожалуйста, введите число от %s до %s.\n"

#: guess.c:71
#, c-format
msgid "Does the number more than %s?\n"
msgstr "Ваше число больше %s?\n"

#: guess.c:80
msgid "yes"
msgstr "да"

#: guess.c:80
msgid "y"
msgstr "д"

#: guess.c:82
msgid "no"
msgstr "нет"

#: guess.c:82
msgid "n"
msgstr "н"

#: guess.c:85
#, c-format
msgid "Bad input! Enter yes or no.\n"
msgstr "Неправильный ввод! Введите да или нет.\n"

#: guess.c:91
#, c-format
msgid "Your number is %s!\n"
msgstr "Ваше число -- %s!\n"
